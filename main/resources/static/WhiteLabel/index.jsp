	<script type="text/javascript">
		$(document).ready(function(){
			$("select").change(function(){
				$(this).find("option:selected").each(function(){
					if($(this).attr("value")=="EMI"){
						$(".councils").not(".EMI").hide();
						$(".EMI").show();
					}
					else if($(this).attr("value")=="PolicyNumber"){
						$(".councils").not(".PolicyNumber").hide();
						$(".PolicyNumber").show();
					}
					else if($(this).attr("value")=="Education"){
						$(".councils").not(".Education").hide();
						$(".Education").show();
					}
					else if($(this).attr("value")=="EMIOthers"){
						$(".councils").not(".EMIOthers").hide();
						$(".EMIOthers").show();
					}
					
				});
			}).change();
		});
		</script>
	
	<script>
	$(document).ready(function(){

	// hide #back-top first
	$("#back-top").hide();
	
	// fade in #back-top
	$(function () {
		$(window).scroll(function () {
			if ($(this).scrollTop() > 100) {
				$('#back-top').fadeIn();
			} else {
				$('#back-top').fadeOut();
			}
		});

		// scroll body to 0px on click
		$('#back-top a').click(function () {
			$('body,html').animate({
				scrollTop: 0
			}, 800);
			return false;
		});
	});

});
</script>
