<script type="text/javascript">

function SetTypeText(number)
{
var typeField = document.getElementById("cardType");
typeField.innerHTML = GetCardType(number);
}
        function GetCardType(number)
       {            
           var re = new RegExp("^4");
           if (number.match(re) != null)
               return '<span class="visa-card"></span>';

           re = new RegExp("^(34|37)");
if (number.match(re) != null)	
               return '<span class="american-card"></span>';

           re = new RegExp("^5[1-5]");
           if (number.match(re) != null)
               return '<span class="master-card"></span>';

           re = new RegExp("^6011");
           if (number.match(re) != null)
               return '<span class="rupay-card"></span>'; 
           return "";
       }
</script>



